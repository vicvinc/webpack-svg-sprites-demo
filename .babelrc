{
  "presets": ["@babel/preset-env"],
  "plugins": [
    "transform-vue-jsx",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-object-assign"
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env"]]
    },
    "production": {
      "presets": ["minify"]
    }
  }
}
